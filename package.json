{
  "name": "ember-milligram",
  "version": "0.3.0",
  "description": "An Ember CLI addon to integrate the Milligram CSS framework into Ember applications",
  "keywords": [
    "ember-addon",
    "milligram",
    "css",
    "framework"
  ],
  "license": "MIT",
  "author": "Alex DiLiberto <npm@alexdiliberto.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexdiliberto/ember-milligram"
  },
  "bugs": {
    "url": "https://github.com/alexdiliberto/ember-milligram/issues"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember serve",
    "test": "ember try:each",
    "inspect": "node --inspect --debug-brk node_modules/.bin/ember s",
    "commit": "git-cz",
    "commitmsg": "commitlint -e",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "deploy": "np",
    "version": "yarn changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "ember-cli-babel": "^6.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-angular": "^6.0.2",
    "broccoli-asset-rev": "^2.4.5",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.5",
    "cz-conventional-changelog": "^2.1.0",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~3.0.3",
    "ember-cli-app-version": "^3.1.3",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^1.1.0",
    "ember-cli-deploy-git": "^1.3.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.3.0",
    "ember-cli-sass": "^7.1.3",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.1.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^6.0.1",
    "husky": "^0.14.3",
    "loader.js": "^4.2.3",
    "milligram": "^1.3.0",
    "np": "^2.18.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://alexdiliberto.com/ember-milligram"
  }
}
